Point = create()
Point.x = 0
Point.y = 0
Point.__call__ = fn (this, x, y)
    o = create(this)
    o.x = x
    o.y = y
    return o
end

Point.__add__ = fn (this, other)
    return Point(this.x + other.x, this.y + other.y)
end

a = Point(3,5)
b = Point(4,0)
c = a + b
print("Point(x=" + str(c.x) + ", y=" + str(c.y) +")")
